{
  "name": "online-markdown",
  "version": "1.0.0",
  "description": "A online markdown converter specially for Wechat Public formatting.",
  "main": "index.js",
  "scripts": {
    "server": "NODE_ENV=development && webpack-dev-server --host 0.0.0.0 --port 5014 --inline --hot --progress --config ./webpack.config.js",
    "build": "NODE_ENV=production && webpack --progress --profile --color --display-modules --display-chunks --config ./webpack.production.config.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/barretlee/online-markdown.git"
  },
  "keywords": [
    "wechat",
    "markdown",
    "format"
  ],
  "author": "Barret Lee <barret.china@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/barretlee/online-markdown/issues"
  },
  "homepage": "https://github.com/barretlee/online-markdown",
  "dependencies": {
    "ajv": "^6.5.2",
    "copy-webpack-plugin": "~4.0.1",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.5"
  },
  "devDependencies": {
    "css-loader": "^0.26.1",
    "file-loader": "^0.10.0",
    "jsx-loader": "^0.13.2",
    "less": "^2.7.2",
    "less-loader": "^2.2.3",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^4.16.5"
  }
}
